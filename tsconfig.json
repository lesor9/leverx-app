{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "skipLibCheck": true,
      "esModuleInterop": true
    },
    "include": [
      "./src/**/*",
      "config/setupTests.ts"
    ]
  }